{
    "routes": [
        {
            "path": "home",
            "component": "homeComponent",
            "children": [
                {
                    "component": "view_travel_requestsComponent",
                    "path": "''"
                },
                {
                    "component": "new_travel_requestComponent",
                    "path": "new-request"
                }
            ]
        },
        {
            "path": "''",
            "redirectTo": "/home",
            "pathMatch": "full",
            "default": true
        },
        {
            "path": "**",
            "component": "PageNotFoundComponent"
        }
    ]
}
